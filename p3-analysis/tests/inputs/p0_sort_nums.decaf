int nums[10];

def void sort_nums(int len)
{
    bool check;
    check = true;

    if (len > 1)
    {
        while (check)
        {
            int i;
            i = 0;
            check = false;

            while (i < len - 1)
            {
                if (nums[i] > nums[i + 1])
                {
                    int temp;
                    temp = nums[i];
                    nums[i] = nums[i + 1];
                    nums[i + 1] = temp;
                    check = true;
                }

                i = i + 1;
            }
        }
    }
}

def int main()
{
    nums[0] = 15;
    nums[1] = 13;
    nums[2] = 9;
    nums[3] = 5;
    nums[4] = 2;
    sort_nums(5);
    return 0;
}